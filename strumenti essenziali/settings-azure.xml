<settings
        xmlns="http://maven.apache.org/SETTINGS/1.0.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                              https://maven.apache.org/xsd/settings-1.0.0.xsd">
    <servers>
        <server>
            <id>iiab-si</id>
            <username>iiab-si</username>
            <password>TUO_TOKEN_PERSONALE_QUI</password>
        </server>
        <server>
            <id>iiab-hr</id>
            <username>iiab-hr</username>
            <password>TUO_TOKEN_PERSONALE_QUI</password>
        </server>
        <server>
            <id>iiab-hu</id>
            <username>iiab-hu</username>
            <password>TUO_TOKEN_PERSONALE_QUI</password>
        </server>
        <server>
            <id>iiab-sp</id>
            <username>iiab-sp</username>
            <password>TUO_TOKEN_PERSONALE_QUI</password>
        </server>
        <server>
            <id>iiab-ch</id>
            <username>iiab-ch</username>
            <password>TUO_TOKEN_PERSONALE_QUI</password>
        </server>
        <server>
            <id>iiab-pt</id>
            <username>iiab-pt</username>
            <password>TUO_TOKEN_PERSONALE_QUI</password>
        </server>
        <server>
            <id>iiab-gel</id>
            <username>iiab-gel</username>
            <password>TUO_TOKEN_PERSONALE_QUI</password>
        </server>
        <server>
            <id>iiab-rgi-bridge</id>
            <username>iiab-rgi-bridge</username>
            <password>TUO_TOKEN_PERSONALE_QUI</password>
        </server>
    </servers>
    <profiles>
        <profile>
            <id>maven-central</id>
            <activeByDefault>true</activeByDefault>
            <repositories>
                <repository>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                    <id>central</id>
                    <url>http://repo1.maven.org/maven2</url>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                    <id>central</id>
                    <url>http://repo1.maven.org/maven2</url>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>
    <mirrors>
        <mirror>
            <id>maven-default-http-blocker</id>
            <mirrorOf>dummy</mirrorOf>
            <name>Dummy mirror to override default blocking mirror that blocks http</name>
            <url>http://0.0.0.0/</url>
            <blocked>true</blocked>
        </mirror>
    </mirrors>
</settings>